{
  // "editor.codeActionsOnSave": {
  //   "source.fixAll": "never"
  // },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#7d23e0",
    "activityBar.background": "#7d23e0",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#df7b20",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#7d23e0",
    "statusBar.background": "#641ab6",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#7d23e0",
    "statusBarItem.remoteBackground": "#641ab6",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#641ab6",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#641ab699",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#641ab6",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  }
